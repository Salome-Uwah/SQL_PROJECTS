SELECT FirstName, LastName, Age,
CASE 
	WHEN Age > 29 THEN 'OLD' 
ELSE 'YOUNG' 
END
FROM [SQL TUTORIAL].[dbo].[EmployeeDemographics]
WHERE Age IS NOT NULL


SELECT FirstName, LastName, Age,
CASE 
	WHEN Age = 35 THEN 'STANLEY' 
	WHEN Age > 29 THEN 'OLD' 
	
ELSE 'YOUNG' 
END
FROM [SQL TUTORIAL].[dbo].[EmployeeDemographics]
WHERE Age IS NOT NULL
ORDER BY Age
